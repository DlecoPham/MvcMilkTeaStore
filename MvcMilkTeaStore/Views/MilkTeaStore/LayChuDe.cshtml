@using MvcMilkTeaStore.Models;
@model IEnumerable<CHUDE>
<div class="title">CHỦ ĐỀ TRÀ SỮA</div>
<ul class="list-group">
    @foreach (var chude in Model)
    {
        <a href="@Url.Action("SPTheoChuDe", "MilkTeaStore", new { id=chude.MaCD })"
           class="list-group-item list-group-item-action">
            @chude.TenChuDe
        </a>
    }
</ul>
